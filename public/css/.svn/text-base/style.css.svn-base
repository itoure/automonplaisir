}


.rss,.facebook,.twitter {
	display: inline-block;
	width: 16px;
	height: 16px;
	margin-right: 10px;
	vertical-align: middle;
	background: url(../image/rft_icons.png) no-repeat
}

.rss {
	background-position: 0 0
}

.facebook {
	background-position: -16px 0
}

.twitter {
	background-position: -32px 0
}

