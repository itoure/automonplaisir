body{
	font:400 12px/1.5 Verdana,Arial,Helvetica,sans-serif	
}

.title-bo{
	font-size:25px;
	border-bottom:2px solid #000;
	margin-bottom:30px;
}

.box-login{
	border:1px solid #000;
	width:500px;
	text-align:center;
	padding-top:30px;
	margin-left: auto;
  	margin-right: auto;
}

.elmt-form{
	margin-bottom:15px;	
}

.labelForm{
	display:inline-block;
	width:150px;
}

.btnSubmit{
	text-align:center;
	margin-top:20px;
}

.center{
	text-align:center;
}

.red{
	color:red;	
}

label.error {
	color:red;
	margin-left:10px;
}

.menu{
	border: 1px solid #000;
	background-color:#F3D617;
	height:50px;
	line-height:4em;
	margin-bottom:40px;
}

.menu a{
	color: #000;
	margin:0 15px;
}

.menu a:hover{
	color: #FFF;
}

.messages{
	margin-bottom:20px;
	text-align:center;
	color:red;
	font-size:15px;
}

.table-contact, .table-rdv, .table-neuf {
	width: 100%;
	margin-bottom:20px;
}

.thead-contact, .thead-rdv, .thead-neuf {
	background-color: #0699FF;
	color:#FFF
}

.tbody-contact, .tbody-rdv, .tbody-neuf {
	text-align: center;
}

.table-contact caption, .table-rdv caption, .table-neuf caption {
	font-size:25px;
}

table {
	border-collapse:collapse;
}

.elmt-form {
	margin-bottom: 5px;
}

div .titre {
	margin:20px 0px;
	font-size:20px;
}